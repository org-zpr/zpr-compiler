# minimal config so that we can test parsing the RFC zpl

[nodes.n0]
provider = [["endpoint.zpr.adapter.cn", "fee"]]
zpr_address = "fd5a:5052:90de::1"

[trusted_services.foo]
prefix = "foo"
api = "validation/2"
returns_attributes = [
  "red -> #service.red", 
  "blue -> #service.blue", 
  "brown -> #service.brown", 
  "green -> #user.green", 
  "yellow -> #endpoint.yellow", 
  "orange -> #endpoint.orange" ]
provider = [["endpoint.zpr.adapter.cn", "foo"]]

[protocols.http]
l4protocol = "TCP"
port = 80

[protocols.dbprot]
l4protocol = "TCP"
port = 9012

[services.foo-client]
protocol = "zpr-oauthrsa"

[services.foo-vs]
protocol = "zpr-validation2"

[services.MyWeb]
protocol = "http"
provider = [[ "endpoint.zpr.adapter.cn", "webserver.zpr" ]]

[services.MyDb]
protocol = "dbprot"
provider = [[ "endpoint.zpr.adapter.cn", "dbserver.zpr" ]]
