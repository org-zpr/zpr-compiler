# minimal config so that we can test parsing the RFC zpl

[nodes.n0]
key = "none"
provider = [["endpoint.zpr.adapter.cn", "fee"]]
zpr_address = "fd5a:5052:90de::1"
interfaces = [ "in1" ]
in1.netaddr = "1.2.3.4:5000"

[visa_service]
dock_node = "n0"

[trusted_services.default]
cert_path = ""

[trusted_services.foo]
prefix = "foo"
api = "validation/2"
returns_attributes = ["#service.red", "#service.blue", "#service.brown", "#user.green", "#endpoint.yellow", "#endpoint.orange" ]
provider = [["endpoint.zpr.adapter.cn", "foo"]]

[protocols.http]
l4protocol = "iana.TCP"
port = 80

[protocols.dbprot]
l4protocol = "iana.TCP"
port = 9012

[services.foo-client]
protocol = "zpr-oauthrsa"

[services.foo-vs]
protocol = "zpr-validation2"

[services.MyWeb]
protocol = "http"
provider = [[ "endpoint.zpr.adapter.cn", "webserver.zpr" ]]

[services.MyDb]
protocol = "dbprot"
provider = [[ "endpoint.zpr.adapter.cn", "dbserver.zpr" ]]
