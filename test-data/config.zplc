# minimal config so that we can test parsing the RFC zpl

[nodes.n0]
key = "none"
provider = [["foo", "fee"]]
zpr_address = "fd5a:5052:90de::1"
interfaces = [ "in1" ]
in1.netaddr = "1.2.3.4:5000"

[visa_service]
dock_node = "n0"

[trusted_services.default]
cert_path = ""

[trusted_services.foo]
prefix = "foo" # maybe prefix should default to <service-name> ?
api = "validation/1"
returns_attributes = [ "clearance", "#government", "service", "mach-type", "svc-id", "#cleared", "machine-id", "#managed", "government", "external-network-connection"]


[protocols.http]
protocol = "iana.TCP"
port = 80

[services.server]
protocol = "http"

[services.Image-database]
protocol = "http"

[services.gateway]
protocol = "http"
