# -*- mode: toml -*-


[resolver]
order = ["hosts", "dns"]

[resolver.hosts]
"node0.zpr" = "fd5a:5052:90de::1"
"node0.overlay" = "192.168.4.69"
"my.service" = "fd5a:5052:1::8080"


[nodes."node.zpr.org"]
provider = [ ["zpr.adapter.cn", "node.zpr.org"]]
zpr_address = "node0.zpr"

[bootstrap]
"bas.zpr.org" = "rsa-pub-key.pem"
"vs.zpr" = "vs.zpr_pub-key.pem"

[trusted_services.default] # note that "default" is special
cert_path = "ca-cert.pem"


[protocols.ping]
protocol = "ICMP6"
icmp_type = "request-response"
icmp_codes = [ 128, 129 ]

[protocols.http]
protocol = "TCP"
port = 80


[services.ZServicePingable]
protocol = "ping"

[services.ZWebService]
protocol = "http"




